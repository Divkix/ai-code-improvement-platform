# JWT Authentication
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Demo User Credentials (automatically created on startup)
# Email: demo@github-analyzer.com
# Password: demo123456

# GitHub OAuth App Configuration
# Create a GitHub OAuth App at https://github.com/settings/applications/new
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# choose b/w local/voyage
EMBEDDING_PROVIDER=voyage
LOCAL_EMBEDDING_URL=http://localhost:1234 ## ignored for local
VECTOR_DIMENSION=1024
LOCAL_EMBEDDING_MODEL=text-embedding-nomic-embed-text-v1.5 ## ignored for local
# Voyage AI API Key
# Get your API key from https://www.voyageai.com/
VOYAGE_API_KEY=your-voyage-api-key

# Anthropic API Key for Claude
# Get your API key from https://console.anthropic.com/
ANTHROPIC_API_KEY=your-anthropic-api-key

# Database Configuration (optional - defaults provided)
MONGODB_URI=mongodb://localhost:27017/github-analyzer
QDRANT_URL=http://localhost:6333

# Development
GIN_MODE=debug
GITHUB_ENCRYPTION_KEY=anyRandomString
